{
  "manifest_version": 2,

  "name": "DRmail",
  "description": "This extension saves you from sending email when not in a good mood.",
  "version": "1.0.1",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
