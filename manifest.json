{
  "name": "DRmail",
  "version": "1.0.1",
  "description": "Never regret sending emails. DRmail stops you from sending emails that might leave a negative impression.",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "jquery.min.js",
    "gmail.js",
    "main.js"
  ],
  "browser_action":{
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
