{
  "name": "Gmail.js",
  "version": "0.1",
  "description": "Hello World Gmail chrome extension",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "jquery.min.js",
    "gmail.js",
    "main.js"
  ],
  "browser_action":{
    "default_icon": "icon.png"
  },
  "manifest_version": 2
}
